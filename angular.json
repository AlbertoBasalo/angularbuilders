          }
        }
      }
    },
    "domain-search-box": {
      "projectType": "library",
      "root": "libs/domain/search-box",
      "sourceRoot": "libs/domain/search-box/src",
      "prefix": "ab",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
