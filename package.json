{
  "name": "angularbuilders",
  "version": "0.0.9",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && npm run build:ssr",
    "nx": "nx",
    "pre_start": "npm run build:ssr",
    "start": "node ./dist/www/server/main.js",
    "build": "ng build --prod",
    "test": "",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev": "ng serve -o --hmr",
    "dev:ssr": "ng run www:serve-ssr",
    "serve:ssr": "node dist/www/server/main.js",
    "build:ssr": "ng build --prod && ng run www:server:production",
    "prerender": "ng run www:prerender",
    "prerelease": "standard-version ",
    "release": "git push --follow-tags origin main",
    "predeploy": "npm run release",
    "deploy": "git checkout heroku-www && git merge main && git push && git checkout main "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@angular/service-worker": "^11.2.0",
    "@nguniversal/express-engine": "^11.2.1",
    "@nrwl/angular": "11.5.2",
    "bulma": "^0.9.2",
    "express": "^4.15.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/template-parser": "~1.0.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@nguniversal/builders": "^11.2.1",
    "@nrwl/cli": "11.5.2",
    "@nrwl/linter": "11.5.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "11.5.2",
    "@nrwl/workspace": "11.5.2",
    "@types/express": "^4.17.0",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "dotenv": "8.2.0",
    "ng-packagr": "^11.0.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.3"
  }
